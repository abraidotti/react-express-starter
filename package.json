{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "A React front-end and Node/Express back-end starter.",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraidotti/react-express-starter.git"
  },
  "keywords": [
    "react",
    "express"
  ],
  "author": "Alessandro Braidotti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abraidotti/react-express-starter/issues"
  },
  "homepage": "https://github.com/abraidotti/react-express-starter#readme",
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.17.1"
  }
}
